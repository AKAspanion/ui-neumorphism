{
  "name": "ui-neumorphism",
  "version": "1.1.0",
  "description": " React components library for neumorphism UI/UX trend.",
  "author": "AKAspanion",
  "license": "MIT",
  "repository": "AKAspanion/ui-neumorphism",
  "keywords": [
    "ui",
    "react",
    "reactjs",
    "react-js",
    "neumorphic",
    "neumorphism",
    "skeuomorphism",
    "ui-neumorphism",
    "neumorphism-ui"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "module": "dist/index.modern.js",
  "source": "src/index.js",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "microbundle-crl --no-compress --format modern,cjs && npm run build:types",
    "start": "microbundle-crl watch --no-compress --format modern,cjs",
    "build:types": "copyfiles -f ./src/index.d.ts dist",
    "prepublish": "run-s build",
    "test": "run-s test:unit test:lint test:build",
    "test:build": "run-s build",
    "test:lint": "eslint .",
    "test:unit": "cross-env CI=1 react-scripts test --env=jsdom",
    "test:watch": "react-scripts test --env=jsdom",
    "predeploy": "cd example && npm install && npm run build",
    "deploy": "gh-pages -d example/build"
  },
  "peerDependencies": {
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "@types/react": "^16.9.43"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.3",
    "copyfiles": "^2.3.0",
    "cross-env": "^7.0.2",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-config-standard-react": "^9.2.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-standard": "^4.0.1",
    "gh-pages": "^2.2.0",
    "microbundle-crl": "^0.13.10",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.4",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "^3.4.1"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "react-transition-group": "^4.4.1"
  }
}
